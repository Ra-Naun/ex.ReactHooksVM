, useEffect } from \"react\";\r\n\r\nfunction complexCompute(num) {\r\n    let i = 0;\r\n    while (i < 599999999) i++;\r\n    return num * 42;\r\n}\r\n\r\nfunction HookUseMemo() {\r\n    const [num, setNum] = useState(1);\r\n    const [colored, setColored] = useState(false);\r\n\r\n    const compNum = useMemo(() => {\r\n        return complexCompute(num);\r\n    }, [num]);\r\n\r\n    const styles = useMemo(() => {\r\n        return {\r\n            color: colored ? \"darkred\" : \"black\",\r\n        };\r\n    }, [colored]);\r\n\r\n    useEffect(() => {\r\n        console.log(styles);\r\n    }, [styles]);\r\n\r\n    function increment() {\r\n        setNum((preCount) => preCount + 1);\r\n    }\r\n\r\n    function decrement() {\r\n        setNum((preCount) => preCount - 1);\r\n    }\r\n\r\n    return (\r\n        <>\r\n            <h6>useMemo</h6>\r\n            <hr />\r\n\r\n            <button onClick={increment} className=\"btn btn-success el\">\r\n                Increment\r\n            </button>\r\n            <button onClick={decrement} className=\"btn btn-danger el\">\r\n                Decrement\r\n            </button>\r\n            <button onClick={() => setColored((prev) => !prev)} className=\"btn btn-info el\">\r\n                Colored\r\n            </button>\r\n            <h1 className=\"text-center\" style={styles}>\r\n                Счетчик: {num}\r\n            </h1>\r\n        </>\r\n    );\r\n}\r\n\r\nexport default HookUseMemo;\r\n","C:\\Users\\Dmitry\\Desktop\\ReactProject\\react-hooks-app\\src\\hooksExamples\\useCallback\\main.js",[],"C:\\Users\\Dmitry\\Desktop\\ReactProject\\react-hooks-app\\src\\hooksExamples\\useCallback\\itemsList.js",[],"C:\\Users\\Dmitry\\Desktop\\ReactProject\\react-hooks-app\\src\\hooksExamples\\useContext\\main.js",[],"C:\\Users\\Dmitry\\Desktop\\ReactProject\\react-hooks-app\\src\\hooksExamples\\useContext\\someBox.js",[],"C:\\Users\\Dmitry\\Desktop\\ReactProject\\react-hooks-app\\src\\hooksExamples\\useContext\